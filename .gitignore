# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

# Ignore Mac DS_Stores
.DS_Store

# Heroku says ignore .env. See https://devcenter.heroku.com/articles/getting-started-with-rails3
.env